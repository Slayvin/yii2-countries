{
    "name": "softedge/yii2-countries",
    "description": "Model and migration to add countries to your yii2 app",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "countries", "extension"],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.14"
    },
    "suggest": {
        "agroxy/yii2-multilingual": "Allows multilingual attributes for AR models"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Softedge",
            "email": "sh@softedge.be"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "slayvin\\countries\\": "src/",
            "slayvin\\countries\\migrations\\": "src/migrations/",
            "slayvin\\countries\\models\\": "src/models/"
        }
    },
    "extra": {
        "config-plugin": {
            "web": "src/config/web.php"
        }
    }
}
